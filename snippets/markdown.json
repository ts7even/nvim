{
  "Note Metadata": {
    "prefix": "meta",
    "body": [
      "---",
      "title: ${1:Note Title}",
      "date: ${2:${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}}",
      "tags: [${3:tag1, tag2}]",
      "category: ${4:notes}",
      "author: ${5:Your Name}",
      "status: ${6|draft,review,published|}",
      "---",
      "",
      "# ${1:Note Title}",
      "",
      "${0:Your content here...}"
    ],
    "description": "Creates note metadata with frontmatter"
  },
  "Daily Note": {
    "prefix": "daily",
    "body": [
      "---",
      "title: Daily Note - ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "type: daily",
      "tags: [daily, journal]",
      "---",
      "",
      "# Daily Note - ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "",
      "## 📅 Today's Focus",
      "- ${1:Main priority for today}",
      "",
      "## ✅ Tasks",
      "- [ ] ${2:Task 1}",
      "- [ ] ${3:Task 2}",
      "- [ ] ${4:Task 3}",
      "",
      "## 📝 Notes",
      "${5:Important notes or observations}",
      "",
      "## 🎯 Goals",
      "- ${6:Goal 1}",
      "- ${7:Goal 2}",
      "",
      "## 🌟 Wins",
      "- ${8:What went well today}",
      "",
      "## 🔄 Tomorrow",
      "- ${9:What to focus on tomorrow}",
      "",
      "## 💭 Reflection",
      "${0:End of day thoughts...}"
    ],
    "description": "Creates a structured daily note template"
  },
  "Book Setup": {
    "prefix": "book",
    "body": [
      "---",
      "title: ${1:Book Title}",
      "author: ${2:Author Name}",
      "isbn: ${3:ISBN}",
      "publisher: ${4:Publisher}",
      "publication_year: ${5:Year}",
      "genre: ${6:Genre}",
      "subject: ${7:Subject/Topic}",
      "pages: ${8:Number of pages}",
      "language: ${9:English}",
      "format: ${10|physical,ebook,audiobook|}",
      "status: ${11|to-read,reading,completed,dnf|}",
      "rating: ${12:0-5}",
      "date_started: ${13:YYYY-MM-DD}",
      "date_finished: ${14:YYYY-MM-DD}",
      "tags: [${15:books, reading}]",
      "---",
      "",
      "# ${1:Book Title}",
      "*by ${2:Author Name}*",
      "",
      "## 📖 Book Information",
      "- **Author:** ${2:Author Name}",
      "- **Genre:** ${6:Genre}",
      "- **Pages:** ${8:Number of pages}",
      "- **Published:** ${5:Year} by ${4:Publisher}",
      "- **ISBN:** ${3:ISBN}",
      "",
      "## 📋 Summary",
      "${16:Brief summary of the book...}",
      "",
      "## 🎯 Key Themes",
      "- ${17:Theme 1}",
      "- ${18:Theme 2}",
      "- ${19:Theme 3}",
      "",
      "## 💡 Key Insights",
      "- ${20:Insight 1}",
      "- ${21:Insight 2}",
      "- ${22:Insight 3}",
      "",
      "## 📝 Notes & Quotes",
      "",
      "### Chapter 1: ${23:Chapter Title}",
      "> \"${24:Notable quote}\" - Page ${25:XX}",
      "",
      "${26:Your notes here...}",
      "",
      "## 🤔 Personal Reflection",
      "${27:What did you think about this book? How did it impact you?}",
      "",
      "## 🔗 Related",
      "- ${28:Related books, articles, or concepts}",
      "",
      "## ⭐ Rating: ${12:0}/5",
      "${0:Why did you give it this rating?}"
    ],
    "description": "Creates a comprehensive book review and note-taking template"
  },
  "Meeting Notes": {
    "prefix": "meeting",
    "body": [
      "---",
      "title: ${1:Meeting Title}",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "time: ${2:Time}",
      "attendees: [${3:Person 1, Person 2}]",
      "type: meeting",
      "tags: [meetings, work]",
      "---",
      "",
      "# ${1:Meeting Title}",
      "",
      "**Date:** ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}  ",
      "**Time:** ${2:Time}  ",
      "**Attendees:** ${3:Person 1, Person 2}  ",
      "",
      "## 🎯 Agenda",
      "1. ${4:Agenda item 1}",
      "2. ${5:Agenda item 2}",
      "3. ${6:Agenda item 3}",
      "",
      "## 📝 Discussion Points",
      "- ${7:Discussion point 1}",
      "- ${8:Discussion point 2}",
      "",
      "## ✅ Action Items",
      "- [ ] ${9:Action item 1} - @${10:assignee} - Due: ${11:date}",
      "- [ ] ${12:Action item 2} - @${13:assignee} - Due: ${14:date}",
      "",
      "## 📋 Decisions Made",
      "- ${15:Decision 1}",
      "- ${16:Decision 2}",
      "",
      "## 🔄 Next Steps",
      "${0:What happens next...}"
    ],
    "description": "Creates a structured meeting notes template"
  },
  "Project Template": {
    "prefix": "project",
    "body": [
      "---",
      "title: ${1:Project Name}",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "status: ${2|planning,active,on-hold,completed|}",
      "priority: ${3|low,medium,high,critical|}",
      "tags: [${4:project, work}]",
      "type: project",
      "---",
      "",
      "# ${1:Project Name}",
      "",
      "## 🎯 Objective",
      "${5:What is the main goal of this project?}",
      "",
      "## 📋 Requirements",
      "- ${6:Requirement 1}",
      "- ${7:Requirement 2}",
      "- ${8:Requirement 3}",
      "",
      "## 👥 Stakeholders",
      "- **Project Manager:** ${9:Name}",
      "- **Team Members:** ${10:Names}",
      "- **Client/Sponsor:** ${11:Name}",
      "",
      "## 📅 Timeline",
      "- **Start Date:** ${12:YYYY-MM-DD}",
      "- **End Date:** ${13:YYYY-MM-DD}",
      "- **Key Milestones:**",
      "  - ${14:Milestone 1}: ${15:Date}",
      "  - ${16:Milestone 2}: ${17:Date}",
      "",
      "## 📊 Progress",
      "- [x] ${18:Completed task}",
      "- [ ] ${19:In progress task}",
      "- [ ] ${20:Pending task}",
      "",
      "## 🚧 Risks & Issues",
      "- ${21:Risk/Issue 1}",
      "- ${22:Risk/Issue 2}",
      "",
      "## 📝 Notes",
      "${0:Additional project notes...}"
    ],
    "description": "Creates a comprehensive project planning template"
  }
}
